<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd"
       default-lazy-init="true">

    <bean id="singerOne"
          class="com.gonzajf.spring.ch4.Singer"
          init-method="init" p:name="John Mayer"  p:age="39"/>

    <bean id="singerTwo"
          class="com.gonzajf.spring.ch4.Singer"
          init-method="init" p:age="72"/>

    <bean id="singerThree"
          class="com.gonzajf.spring.ch4.Singer"
          init-method="init" p:name="John Butler"/>


    <bean id="singerWithInterfaceOne"
          class="com.gonzajf.spring.ch4.SingerWithInterface"
          p:name="John Mayer"  p:age="39"/>

    <bean id="singerWithInterfaceTwo"
          class="com.gonzajf.spring.ch4.SingerWithInterface"
          p:age="72"/>

    <bean id="singerWithInterfaceThree"
          class="com.gonzajf.spring.ch4.SingerWithInterface"
          p:name="John Butler"/>

</beans>